{
  "name": "Meta Identity",
  "description": "Aggregates a number of identity providers together.",
  "app": {
    "script": ["identity.js"]
  },
  "dependencies": {
    "loopbackIdentity": {
      "url": "common/backend/identity/loopback/identity.loopback.json",
      "api": "identity"
    },
    "manualIdentity": {
      "url": "common/backend/identity/manual/identity.manual.json",
      "api": "identity"
    },
    "googleIdentity": {
      "url": "common/backend/identity/xmpp/google/identity.google.json",
      "api": "identity"
    }
  },
  "provides": [
    "identity"
  ],
  "permissions": [
    "core.view"
  ]
}
