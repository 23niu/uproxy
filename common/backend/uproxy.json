{
  "name": "UProxy",
  "description": "The UProxy backend provides a mesh web proxy, access the web through your social connections.",
  "app": {
    "script": [
      "util.js",
      "nouns-and-adjectives.js",
      "constants.js",
      "state-storage.js",
      "uproxy.js",
      "start-uproxy.js",
      "test/uproxy_local_test_proxying.js"
    ]
  },
  "dependencies": {
    "uproxyclient": {
      "url": "client/client.json"
    },
    "uproxyserver": {
      "url": "server/server.json"
    },
    "storage": {
      "url": "storage/storage.json",
      "api": "storage"
    }
    "identity": {
      "url": "social/identity.json",
      "api": "identity"
    },
  },
  "permissions": [
    "core.sctp-peerconnection"
  ]
}
