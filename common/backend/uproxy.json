{
  "name": "UProxy",
  "description": "The UProxy backend provides a mesh web proxy, access the web through your social connections.",
  "app": {
    "script": ["nouns-and-adjectives.js", "util.js", "uproxy.js"]
  },
  "dependencies": {
    "uproxyclient": {
      "url": "client/client.json"
    },
    "uproxyserver": {
      "url": "server/server.json"
    },
    "identity": {
      "url": "identity/identity.json",
      "api": "identity"
    },
    "storage": {
      "url": "storage/storage.json",
      "api": "storage"
    }
  },
  "permissions": [
    "core.sctp-peerconnection"
  ]
}
