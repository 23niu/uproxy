{
  "name": "UProxy",
  "description": "The UProxy backend provides a mesh web proxy, access the web through your social connections.",
  "app": {
    "script": [
      "uproxy.js",
      "view_oauth.js",
      "util.js",
      "auth.js",
      "consent.js",
      "nouns-and-adjectives.js",
      "storage.js",
      "local-instance.js",
      "remote-instance.js",
      "user.js",
      "social-enum.js",
      "social.js",
      "core.js"
    ]
  },
  "dependencies": {
    "SocksToRtc": {
      "url": "../lib/socks-rtc/socks-to-rtc/socks-to-rtc.json"
    },
    "RtcToNet": {
      "url": "../lib/socks-rtc/rtc-to-net/rtc-to-net.json"
    },
    "SOCIAL-google": {
      "url": "../lib/freedom-social-xmpp/social.google.json",
      "api": "social"
    },
    "storage": {
      "url": "../lib/storage/storage.isolated.json",
      "api": "storage"
    }
  },
  "permissions": [
    "core.peerconnection",
    "core.tcpsocket",
    "core.udpsocket"
  ]
}
