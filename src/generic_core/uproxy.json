{
  "name": "UProxy",
  "description": "The UProxy backend provides a mesh web proxy, access the web through your social connections.",
  "app": {
    "script": [
      "concat_core_script.js"
    ]
  },
  "dependencies": {
    "SocksToRtc": {
      "url": "../lib/socks-rtc/socks-to-rtc/socks-to-rtc.json"
    },
    "RtcToNet": {
      "url": "../lib/socks-rtc/rtc-to-net/rtc-to-net.json"
    },
    "SOCIAL-google": {
      "url": "../lib/freedom-social-xmpp/social.google.json",
      "api": "social"
    },
    "storage": {
      "url": "../lib/storage/storage.isolated.json",
      "api": "storage"
    }
  },
  "permissions": [
    "core.peerconnection",
    "core.tcpsocket",
    "core.udpsocket"
  ]
}
