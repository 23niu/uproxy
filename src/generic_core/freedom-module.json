{
  "name": "uProxy",
  "description": "The uProxy backend provides a mesh web proxy. Access the web through your social connections.",
  "app": {
    "script": [
      "freedom-module.static.js"
    ]
  },
  "dependencies": {
    "SOCIAL-Google": {
      "url": "../lib/freedom-social-xmpp/social.google.json",
      "api": "social"
    },
    "SOCIAL-Facebook": {
      "url": "../lib/freedom-social-firebase/social.firebase-facebook.json",
      "api": "social"
    },
    "storage": {
      "url": "../lib/storage/storage.shared.json",
      "api": "storage"
    },
    "loggingprovider": {
      "url": "uproxy-lib/loggingprovider/loggingprovider.json",
      "api": "loggingprovider"
    }
  },
  "permissions": [
    "core.rtcpeerconnection",
    "core.rtcdatachannel",
    "core.tcpsocket",
    "core.udpsocket",
    "core.xhr"
  ]
}
