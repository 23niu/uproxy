{
  "name": "__MSG_appName__",
  "version": "0.4.0",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "minimum_chrome_version": "38",
  "icons": {
    "16": "icons/uproxy-16.png",
    "19": "icons/uproxy-19.png",
    "128": "icons/uproxy-128.png"
  },
  "default_locale": "en",
  "background": {
    "persistent": true,
    "scripts": [
      "scripts/chrome_glue.js",
      "scripts/uproxy.js",
      "scripts/user.js",
      "scripts/ui.js",
      "scripts/chrome_browser_api.js",
      "scripts/core_connector.js",
      "scripts/oauth.js",
      "scripts/chrome_tab_auth.js",
      "scripts/facebook_auth.js",
      "scripts/google_auth.js",
      "scripts/chrome_connector.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/uproxy-19.png"
    },
    "default_title": "__MSG_appName__",
    "default_popup": "polymer/popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
    "proxy",
    "notifications",
    "https://www.uproxy.org/oauth-redirect-uri*",
    "webRequest",
    "webRequestBlocking",
    "https://graph.facebook.com/oauth/access_token",
    "https://accounts.google.com/o/oauth2/token"
  ],
  "options_page": "polymer/popup.html",
  "key": ""
}
